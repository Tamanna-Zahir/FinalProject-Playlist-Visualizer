<!DOCTYPE html>
<html>
<head>
<%- include("partials/head.ejs") %>
</head>
<body>
  <%- include("partials/nav.ejs") %>
  <h1> <%= songs[0].name %> </h1>

    <Table>
      <tr>
        <th>Song Name</th>
        <th>Artist</th>
        <th>Genre</th>
        <th>Update/Delete</th>
      </tr>
         <% for (const song of songs) { %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td><%= song.genre %></td>
        <td>[<a href="/updateSong?songId=<%=song.songId %>">Update</a>]
        [<a href="/deleteSong?songId=<%=song.songId %>">Delete</a>]</td>
      </tr>
      <% } %>
    </Table>



    <%- include("partials/footer.ejs") %>
</body>
</html>